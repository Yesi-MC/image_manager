<div class='jumbotron jumbotron-fluid'>
  <div class='container'>
    <h1 class='display-4'>Welcome to Image Manager</h1>
    <p class='lead'>This is an application to explore and upload images! Please login to use the app </p>
  </div>
</div> 

<%= form_with url: login_path, method: :get, local: true do |f|%>
    <%= f.label :email, 'Email' %>
    <%= f.email_field :email %>
    <%= f.label :password, 'Password' %>
    <%= f.password_field :password %><br/><br/>
    <%= f.submit "Login" %>
<% end %>